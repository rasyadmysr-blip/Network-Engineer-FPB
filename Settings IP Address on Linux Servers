cd /etc/netplan
ls
sudo nano 00-installer-config.yaml

network:
  ethernets:
    enp0s3:   #sesuaikan dgn interface
      dhcp4: false
      addresses: [192.168.1.50/24]    #sesuaikan dengan IP Address yg diinginkan
      gateway4: 192.168.1.1          #sesuaikan dengan IP Gateway yang diinginkan
      nameservers:
        adressess: [118.98.44.100,8.8.8.8]
  version: 2

sudo netplan apply
